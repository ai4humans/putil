# .coveragerc to control coverage.py during non-tox and continuous integration runs
[run]
include = putil/eng.py
          putil/exdoc.py
          putil/exdoc.py
          putil/exh.py
          putil/misc.py
          putil/pinspect.py
          putil/pcontracts.py
          putil/pcsv.py
          putil/plot.py
          putil/test.py
          putil/tree.py

[xml]
output = shippable/codecoverage/coverage.xml
